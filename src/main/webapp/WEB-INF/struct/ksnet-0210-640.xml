<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<struct>
	<!-- KS-NET 거래내역 통지 결번 -->
	<!-- 공통부 -->
	<field name="H_GP_CD"		length="9"		type="C"	response="Y"	default=""/>			<!-- 식별코드(기업(TXEB9KSV), 국민(업체코드와 동일), 외환(REALTIME), 농협(SRS1), 씨티(JCKC),전북(DONKSVN) 그 외 은행은 SPACE) -->
	<field name="H_FBS_CD"		length="8"		type="C"	response="Y"	default=""/>			<!-- 업체코드 -->
	<field name="H_BNK_CD2"		length="2"		type="C"	response="Y"	default=""/> 			<!-- 은행코드2 -->
	<field name="H_MSG_CD"		length="4"		type="C"	response="Y"	default="0210"/>		<!-- 메시지코드 -->
	<field name="H_SVR_CD"		length="3"		type="C"	response="Y"	default="640"/>			<!-- 업무구분코드 -->
	<field name="H_SND_CNT"		length="1"		type="C"	response="Y"	default=""/>			<!-- 송신횟수 -->
	<field name="H_SEQ_NO"		length="6"		type="C"	response="Y"	default=""/>			<!-- 전문번호 -->
	<field name="H_SND_DT"		length="8"		type="C"  	response="Y"	default=""/>			<!-- 전송일자 -->
	<field name="H_SND_TM"		length="6"		type="C"  	response="Y"	default=""/>			<!-- 전송시간 -->			
	<field name="H_RS_CD"		length="4"		type="C"	response="Y"	default=""/> 			<!-- 응답코드 -->
	<field name="BANKRESPCODE"	length="4"		type="C"	response="Y"	default=""/> 			<!-- 은행 응답코드(전문을 전송할 은행(펌뱅킹 계약한 은행)의 응답 코드) -->
	<field name="H_SH_DT"		length="8"		type="C"	response="Y"	default=""/>			<!-- 조회일자 -->
	<field name="H_SH_NO"		length="6"		type="C"	response="Y"	default=""/>			<!-- 조회번호 -->
	<field name="H_BNK_NO"		length="15"		type="C"	response="Y"	default=""/>			<!-- 은행전문번호 -->
	<field name="H_BNK_CD3"		length="3"		type="C"	response="Y"	default=""/>			<!-- 은행코드3(전문을 전송할 은행(펌뱅킹 계약한 은행)의 은행코드) -->
	<field name="H_FLR"			length="13"		type="C"	response="Y"	default=""/>			<!-- 예비3 -->
	
	<!-- 지급이체 개별부 -->	
	<field name="ACC_NO"			length="15"		type="C"	response="Y"	default=""/>			<!-- 계좌번호 -->
	<field name="MAKE_CNT"			length="2"		type="C"	response="Y"	default=""/>			<!-- 조립건수 -->
	<field name="TRS_KN"			length="2"		type="C"	response="Y"	default=""/>			<!-- 거래구분 -->
	<field name="BNK_CD2"			length="2"		type="C"	response="Y"	default=""/>			<!-- 은행코드2 -->
	<field name="TRS_AMT"			length="13"		type="C"	response="Y"	default=""/>			<!-- 금액 -->
	<field name="BLC_AMT"			length="13"		type="C"	response="Y"	default=""/>			<!-- 잔액 -->
	<field name="BRC_CD"			length="6"		type="C"	response="Y"	default=""/>			<!-- 입금지점코드6 -->
	<field name="DSC"				length="14"		type="C"	response="Y"	default=""/>			<!-- 적요 -->
	<field name="CHK_NO"			length="10"		type="C"	response="Y"	default=""/>			<!-- 수표번호 -->
	<field name="CASH"				length="13"		type="C"	response="Y"	default=""/>			<!-- 현금 -->
	<field name="CHK_AMT"			length="13"		type="C"	response="Y"	default=""/>			<!-- 타행수표금액 -->
	<field name="ETC_AMT"			length="13"		type="C"	response="Y"	default=""/>			<!-- 가계수표,기타금액 -->
	<field name="VACC_NO"			length="16"		type="C"	response="Y"	default=""/>			<!-- 가상계좌번호 -->
	<field name="TRS_DT"			length="8"		type="C"	response="Y"	default=""/>			<!-- 거래일자 -->
	<field name="TRS_TM"			length="6"		type="C"	response="Y"	default=""/>			<!-- 거래시간 -->
	<field name="BKTR_NO"			length="6"		type="C"	response="Y"	default=""/>			<!-- 통장거래 일련번호 -->
	<field name="BNK_CD3"			length="3"		type="C"	response="Y"	default=""/>			<!-- 은행코드3 -->
	<field name="BRN_CD"			length="7"		type="C"	response="Y"	default=""/>			<!-- 입금지점코드7 -->
	<field name="FLR"				length="38"		type="C"	response="Y"	default=""/>			<!-- 예비 -->
</struct>

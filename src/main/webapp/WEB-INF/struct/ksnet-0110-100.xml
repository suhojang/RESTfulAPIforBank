<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<struct>
	<!-- KS-NET 지급이체 응답 -->
	<!-- 공통부 -->
	<field name="H_GP_CD"		length="9"		type="C"	response="Y"	default=""/>			<!-- 식별코드(기업(TXEB9KSV), 국민(업체코드와 동일), 외환(REALTIME), 농협(SRS1), 씨티(JCKC),전북(DONKSVN) 그 외 은행은 SPACE) -->
	<field name="H_FBS_CD"		length="8"		type="C"	response="Y"	default=""/>			<!-- 업체코드 -->
	<field name="H_BNK_CD2"		length="2"		type="C"	response="Y"	default=""/> 			<!-- 은행코드2 -->
	<field name="H_MSG_CD"		length="4"		type="C"	response="Y"	default="0110"/>		<!-- 메시지코드 -->
	<field name="H_SVR_CD"		length="3"		type="C"	response="Y"	default="100"/>			<!-- 업무구분코드 -->
	<field name="H_SND_CNT"		length="1"		type="C"	response="Y"	default=""/>			<!-- 송신횟수 -->
	<field name="H_SEQ_NO"		length="6"		type="C"	response="Y"	default=""/>			<!-- 전문번호 -->
	<field name="H_SND_DT"		length="8"		type="C"  	response="Y"	default=""/>			<!-- 전송일자 -->
	<field name="H_SND_TM"		length="6"		type="C"  	response="Y"	default=""/>			<!-- 전송시간 -->			
	<field name="H_RS_CD"		length="4"		type="C"	response="Y"	default=""/> 			<!-- 응답코드 -->
	<field name="BANKRESPCODE"	length="4"		type="C"	response="Y"	default=""/> 			<!-- 은행 응답코드(전문을 전송할 은행(펌뱅킹 계약한 은행)의 응답 코드) -->
	<field name="H_SH_DT"		length="8"		type="C"	response="Y"	default=""/>			<!-- 조회일자 -->
	<field name="H_SH_NO"		length="6"		type="C"	response="Y"	default=""/>			<!-- 조회번호 -->
	<field name="H_BNK_NO"		length="15"		type="C"	response="Y"	default=""/>			<!-- 은행전문번호 -->
	<field name="H_BNK_CD3"		length="3"		type="C"	response="Y"	default=""/>			<!-- 은행코드3(전문을 전송할 은행(펌뱅킹 계약한 은행)의 은행코드) -->
	<field name="H_FLR"			length="13"		type="C"	response="Y"	default=""/>			<!-- 예비3 -->
	
	<!-- 지급이체 응답 개별부 -->	
	<field name="OUT_ACC"			length="15"		type="C"	response="Y"	default=""/>			<!-- 출금계좌번호 -->
	<field name="OUT_ACCPW"			length="8"		type="C"	response="Y"	default=""/>			<!-- 통장 비밀번호 -->
	<field name="REC_CD"			length="6"		type="C"	response="Y"	default=""/>			<!-- 복기부호 -->
	<field name="OUT_AMT"			length="13"		type="C"	response="Y"	default=""/>			<!-- 출금 금액 -->
	<field name="BLC_CD"			length="1"		type="C"	response="Y"	default=""/>			<!-- 출금 후 잔액부호 -->
	<field name="BALANCE"			length="13"		type="C"	response="Y"	default=""/>			<!-- 출금 후 잔액 -->
	<field name="BNK_CD2"			length="2"		type="C"	response="Y"	default=""/>			<!-- 입금 은행코드2 -->
	<field name="IN_ACC"			length="15"		type="C"	response="Y"	default=""/>			<!-- 입금 계좌번호 -->
	<field name="FEE"				length="9"		type="C"	response="Y"	default=""/>			<!-- 수수료 -->
	<field name="TRX_TM"			length="6"		type="C"	response="Y"	default=""/>			<!-- 이체 시각 -->
	<field name="IN_DSC"			length="20"		type="C"	response="Y"	default=""/>			<!-- 입금 계좌 적요 -->
	<field name="CMD_CD"			length="16"		type="C"	response="Y"	default=""/>			<!-- CMS코드 -->
	<field name="RNO"				length="13"		type="C"	response="Y"	default=""/>			<!-- 신원확인번호 -->
	<field name="ATAC_KN"			length="2"		type="C"	response="Y"	default=""/>			<!-- 자동이체 구분 -->
	<field name="OUT_DSC"			length="20"		type="C"	response="Y"	default=""/>			<!-- 출금 계좌 적요 -->
	<field name="IN_BNK"			length="3"		type="C"	response="Y"	default=""/>			<!-- 입금 은행코드3 -->
	<field name="SLR_YN"			length="1"		type="C"	response="Y"	default=""/>			<!-- 급여 구분 -->
	<field name="FLR"				length="37"		type="C"	response="Y"	default=""/>			<!-- 예비 -->
</struct>
